{
  "family": "echo-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::282764120287:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "echo",
      "image": "ealen/echo-server:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}